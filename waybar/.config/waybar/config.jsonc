{
    "layer": "top", // Waybar at top layer
    "position": "bottom", // Waybar position (top|bottom|left|right)
    "height": 30, // Waybar height (to be removed for auto height)
    "spacing": 4, // Gaps between modules (4px)
    "modules-left": [
        "sway/workspaces",
        "custom/updates"
    ],
    "modules-center": [
        "sway/window"
    ],
    "modules-right": [
        "pulseaudio",
        "cpu",
        "memory",
        "temperature",
        "battery",
        "tray",
        "clock"
    ],
    "sway/workspaces": {
        "disable-scroll": true,
        "on-click": "activate",
        "format": "{name}",
        "on-scroll-up": "swaymsg workspace prev",
        "on-scroll-down": "swaymsg workspace next",
        "format-icons": {
            "1": "ï„ ",
            "2": "ï‰¨",
            "3": "ï„¡",
            "4": "ï†¼",
            "5": "ïµ",
            "urgent": "ïª",
            "focused": "ï†’",
            "default": "ï„‘"
        }
    },
    "keyboard-state": {
        "numlock": false,
        "capslock": false,
        "format": "{name} {icon}",
        "format-icons": {
            "locked": "ï€£",
            "unlocked": "ï‚œ"
        }
    },
    "sway/window": {
        "max-length": 50,
