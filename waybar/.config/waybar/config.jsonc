        "unknown-tag": "N/A",
        "interval": 2,
        "consume-icons": {
            "on": "ïƒ„ "
        },
        "random-icons": {
            "off": "<span color=\"#f53c3c\">ï´</span> ",
            "on": "ï´ "
        },
        "repeat-icons": {
            "on": "ï€ "
        },
        "single-icons": {
            "on": "ï€1 "
        },
        "state-icons": {
            "paused": "ïŒ",
            "playing": "ï‹"
        },
        "tooltip-format": "MPD (connected)",
        "tooltip-format-disconnected": "MPD (disconnected)"
    },
    "idle_inhibitor": {
        "format": "{icon}",
        "format-icons": {
            "activated": "ï®",
            "deactivated": "ï°"
